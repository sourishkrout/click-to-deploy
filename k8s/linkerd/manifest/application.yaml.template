---
apiVersion: app.k8s.io/v1beta1
kind: Application
metadata:
  name: "$APP_INSTANCE_NAME"
  namespace: "$NAMESPACE"
  annotations:
    kubernetes-engine.cloud.google.com/icon: >-
      data:image/png;base64,
    marketplace.cloud.google.com/deploy-info: '{partner_id: "click-to-deploy-containers", product_id: "linkerd", partner_name: "Google Click to Deploy"}'
  labels:
    app.kubernetes.io/name: "$APP_INSTANCE_NAME"
spec:
  descriptor:
    type: linkerd
    version: 'stable-2.0.0'
    description: >-
      Linkerd gives you instant Grafana dashboards and CLI debugging tools for any
      Kubernetes service â€” with no cluster-wide installation.

      To learn more about linkerd, visit the [linkerd website](https://linkerd.io/).
    maintainers:
    - name: Google Click to Deploy
      url: https://cloud.google.com/solutions/#click-to-deploy
    links:
    - description: 'User Guide: Google Click to Deploy Linkerd'
      url: https://github.com/GoogleCloudPlatform/click-to-deploy/blob/master/k8s/linkerd/README.md
    - description: Getting Started with Linkerd
      url: https://linkerd.io/2/getting-started/
  info:
  - name: linkerd dashboard
    type: Reference
    valueFrom:
      serviceRef:
        name: web
  selector:
    matchLabels:
      app.kubernetes.io/name: "$APP_INSTANCE_NAME"
  componentKinds:
  - group: rbac.authorization.k8s.io/v1beta1
    kind: ClusterRoleBinding
  - group: rbac.authorization.k8s.io/v1beta1
    kind: ClusterRole
  - group: v1
    kind: ServiceAccount
  - group: v1
    kind: Service
  - group: extensions/v1beta1
    kind: Deployment
  - group: v1
    kind: Namespace
  - group: v1
    kind: ConfigMap

---
